{
  "name": "two-phat",
  "description": "The default theme for Two Phat Dacks web site",
  "version": "0.0.5",
  "engines": {
    "ghost": ">=2.0.0",
    "ghost-api": "v2"
  },
  "license": "MIT",
  "author": {
    "email": "troy@dack.com.au",
    "name": "Troy Dack",
    "url": "https://twophat.dack.com.au"
  },
  "scripts": {
    "publish": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then git push production; else echo \"Uncomitted changes found.\" && exit 1; fi"
  },
  "gpm": {
    "type": "theme",
    "categories": [
      "Recipe"
    ]
  },
  "keywords": [
    "ghost",
    "ghost-theme",
    "theme"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tdack/twophat"
  },
  "config": {
    "posts_per_page": 6,
    "image_sizes": {
      "xxs": {
        "width": 30
      },
      "xs": {
        "width": 100
      },
      "s": {
        "width": 300
      },
      "m": {
        "width": 600
      },
      "l": {
        "width": 1000
      },
      "xl": {
        "width": 2000
      }
    }
  },
  "dependencies": {}
}
